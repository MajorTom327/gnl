/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	*ft_memchr(const void *s, int c, size_t n)
{
	size_t			index;
	unsigned char	*tmp_s;

	tmp_s = (unsigned char *)s;
	index = 0;
	while (index < n)
	{
		if (tmp_s[index] == (unsigned char)c)
			return (tmp_s + index);
		index++;
	}
	return (0);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_memcmp.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: vthomas <marvin@42.fr>                     +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2015/12/01 04:23:07 by vthomas           #+#    #+#             */
/*   Updated: 2015/12/23 20:44:55 by vthomas          ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int	ft_memcmp(const void *s1, const void *s2, size_t n)
{
	size_t				i;
	const unsigned char	*s1_cpy = (const unsigned char *)s1;
	const unsigned char	*s2_cpy = (const unsigned char *)s2;

	i = 0;
	if (n == 0)
		return (0);
	while (s1_cpy[i] == s2_cpy[i] && n > i)
	{
		if (i == (n - 1))
			return (0);
		i++;
	}
	return ((int)(s1_cpy[i] - s2_cpy[i]));
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_memcpy.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: vthomas <marvin@42.fr>                     +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2015/12/01 01:38:25 by vthomas           #+#    #+#             */
/*   Updated: 2016/01/13 16:25:48 by vthomas          ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	*ft_memcpy(void *dst, const void *src, size_t n)
{
	size_t	index;

	index = 0;
	while (index != n)
	{
		((unsigned char *)dst)[index] = ((unsigned char *)src)[index];
		index++;
	}
	return (dst);
}
