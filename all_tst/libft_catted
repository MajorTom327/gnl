OBJ_PATH = ./obj/
INC_PATH = ./
INC_NAME = libft.h
INC = $(addprefix -I, $(INC_PATH))
CC = gcc
CFLAGS = -Werror -Wall -Wextra
OBJ_NAME = $(SRC_NAME:.c=.o)
SRC = $(addprefix $(SRC_PATH),$(SRC_NAME))
OBJ = $(addprefix $(OBJ_PATH),$(OBJ_NAME))
LIB_NAME = libft.a
LIB_PATH = ./
LIB = $(addprefix $(LIB_PATH),$(LIB_NAME))

all: $(NAME)
$(NAME): $(OBJ) $(LIB)

$(OBJ_PATH)%.o: $(SRC_PATH)%.c
	@mkdir -p $(OBJ_PATH)
	@$(CC) $(CFLAGS) -c $(INC) -o $@ $<

$(LIB):
	@ar rc $@ $(OBJ)
	@ranlib $@

clean:
	@/bin/rm -rf $(OBJ)

fclean: clean
	@/bin/rm -rf $(OBJ_PATH)
	@/bin/rm -rf $(NAME)
	@/bin/rm -rf $(LIB)

re: fclean all

test: re
	@clear
	@$(CC) $(CFLAGS) $(INC) main.c $(LIB)
	@./a.out | cat -e
	@/bin/rm -rf a.out
	@(cd ./ && $(MAKE) fclean)

norme:
	@clear
	@norminette $(SRC) $(INC_PATH)*.h
.PHONY: fclean clean
vthomas
